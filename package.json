{
  "name": "react-boilerplate-with-webpack",
  "version": "0.1.0",
  "description": "react boilerplate with webpack in order to have a straightforward scaffold to practice  ",
  "main": "src/entries/index.js",
  "scripts": {
    "build:dev": "webpack-dev-server --config ./webpack.dev.config.js --open 'google-chrome'",
    "build:local": "webpack --env.NODE_ENV=local",
    "build:prod": "webpack -p --env.NODE_ENV=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklinlugo/react-boilerplate-with-webpack.git"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "Franklin Lugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/franklinlugo/react-boilerplate-with-webpack/issues"
  },
  "homepage": "https://github.com/franklinlugo/react-boilerplate-with-webpack#readme",
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.18",
    "css-loader": "0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "style-loader": "0.20.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
